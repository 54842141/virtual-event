{
  "functions": {
    "predeploy": "npm --prefix \"$RESOURCE_DIR\" run build"
  },
  "rewrites": [
    {
      "source": "/event/**",
      "function": "event"
    },
    {
      "source": "/booth/**",
      "function": "booth"
    },
    {
      "source": "/webinar/**",
      "function": "webinar"
    },
    {
      "source": "/bannerContainers/**",
      "function": "bannerContainers"
    },
    {
      "source": "/banners/**",
      "function": "banners"
    }
  ]
}
